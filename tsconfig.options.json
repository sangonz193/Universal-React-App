{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "incremental": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": true,
    "target": "esnext",

    "jsx": "react-native",
    "allowJs": true,
    "resolveJsonModule": true,
    "jsxImportSource": "nativewind",
    "lib": ["dom", "dom.iterable", "esnext"]
  }
}
